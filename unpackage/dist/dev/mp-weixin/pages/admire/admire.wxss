
.explore_top {
	width: 100%;
	height: 200px;
	background-color: #c6b089;
	position: absolute;
	top: 0;
	left: 0;
	z-index: -999;
}
.explore_font {
	color: #fff;
	font-size: 20px;
	position: absolute;
	left: 50vw;
	-webkit-transform: translate(-50%);
	        transform: translate(-50%);
	top: 25px;
	z-index: 1000;
}
.explore_center {
	margin-top: 80px;
	display: block;
	background-color: #f6f2e6;
	height: 1000px;
	border-radius: 30px 30px 0 0;
}
.explore_lines {
	margin-top: 10px;
	display: flex;
	font-size: 11px;
	padding-top: 30px;
}
.common_imgs {
	width: 20px;
	height: 20px;
}

